@model ReservationViewModel
@{
    ViewData["Title"] = "Details Reservation";
    Layout = "_DashboardLayout";
}
<h1 class="mb-4">Reservation Details</h1>

<div class="text-danger mb-3">@TempData["Error"]</div>

<partial name="Partials/_ReservationPartial" model="Model" />

<div class="d-flex gap-3 mt-3">
    <a asp-action="Edit" asp-route-id="@Model.ResId" class="btn btn-primary">Edit</a>
    <a asp-action="Index" class="btn btn-secondary">Back</a>
</div>
