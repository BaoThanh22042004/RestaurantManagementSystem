@model UserViewModel

<partial name="Partials/_ModalHeader" model='"Details User"' />

<div class="modal-body" id="modalBody">
	<dl>
		<dt>@Html.DisplayNameFor(m => m.UserId)</dt>
		<dd>@Model.UserId</dd>

		<dt>@Html.DisplayNameFor(m => m.Username)</dt>
		<dd>@Model.Username</dd>

		<dt>@Html.DisplayNameFor(m => m.FullName)</dt>
		<dd>@Model.FullName</dd>

		<dt>@Html.DisplayNameFor(m => m.Email)</dt>
		<dd>@Model.Email</dd>

		<dt>@Html.DisplayNameFor(m => m.Phone)</dt>
		<dd>@Model.Phone</dd>

		<dt>@Html.DisplayNameFor(m => m.Role)</dt>
		<dd>@Model.Role</dd>

		<dt>@Html.DisplayNameFor(m => m.Salary)</dt>
		<dd>@Model.Salary</dd>

		<dt>@Html.DisplayNameFor(m => m.IsActive)</dt>
		<dd>@Model.IsActive</dd>
	</dl>
</div>

<partial name="Partials/_ModalFooter" model='""' />