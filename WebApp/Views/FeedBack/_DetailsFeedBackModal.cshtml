@model WebApp.Models.FeedBackViewModel

<partial name="Partials/_ModalHeader" model='"Details Feedback"' />

<div class="modal-body" id="modalBody">
	<dl>
		<dt>@Html.DisplayNameFor(m => m.FeedbackId)</dt>
		<dd>@Model.FeedbackId</dd>

		<dt>@Html.DisplayNameFor(m => m.CreateAt)</dt>
		<dd>@Model.CreateAt</dd>

		<dt>@Html.DisplayNameFor(m => m.FullName)</dt>
		<dd>@Model.FullName</dd>

		<dt>@Html.DisplayNameFor(m => m.Email)</dt>
		<dd>@Model.Email</dd>

		<dt>@Html.DisplayNameFor(m => m.Phone)</dt>
		<dd>@Model.Phone</dd>

		<dt>@Html.DisplayNameFor(m => m.Subject)</dt>
		<dd>@Model.Subject</dd>

		<dt>@Html.DisplayNameFor(m => m.Body)</dt>
		<dd>@Model.Body</dd>

		<dt>@Html.DisplayNameFor(m => m.Status)</dt>
		<dd>@Model.Status</dd>

		<dt>@Html.DisplayNameFor(m => m.Note)</dt>
		<dd>@(Model.Note ?? "N/A")</dd>
	</dl>
</div>

<partial name="Partials/_ModalFooter" model='""' />
