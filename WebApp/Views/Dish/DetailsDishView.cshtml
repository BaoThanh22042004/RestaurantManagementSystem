@model DishViewModel
@{
    ViewData["Title"] = "Details Dish";
    Layout = "_DashboardLayout";
}

<h1 class="mb-4">Dish Details</h1>

<div class="text-danger mb-3">@TempData["Error"]</div>

<partial name="Partials/_DishPartial" model="Model" />

<div class="d-flex gap-3 mt-3">
    <a asp-action="Edit" asp-route-id="@Model.DishId" class="btn btn-primary">Edit</a>
    <a asp-action="Index" class="btn btn-secondary">Back</a>
</div>
