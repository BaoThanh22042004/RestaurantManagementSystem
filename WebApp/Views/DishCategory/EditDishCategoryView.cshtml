@model DishCategoryViewModel
@{
	ViewData["Title"] = "Edit Dish Category";
	Layout = "_DashBoardLayout";
}
<h1 class="mb-4">Edit Dish Category</h1>

<div class="text-danger mb-3">@TempData["Error"]</div>

<form asp-action="Edit" asp-route-id="@Model.CategoryId" method="post">
	<partial name="Partials/_CategoryPartial" model="Model" />

	<div class="d-flex gap-3">
		<button type="submit" class="btn btn-primary">Save</button>
		<a asp-action="Index" class="btn btn-secondary">Back</a>
	</div>
</form>