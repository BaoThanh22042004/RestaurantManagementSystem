@model CategoryViewModel
@{
    ViewData["Title"] = "Details Dish Category";
    Layout = "_DashboardLayout";
}
<h1 class="mb-4">Details Dish Category</h1>

<div class="text-danger mb-3">@TempData["Error"]</div>

<partial name="Partials/_CategoryPartial" model="Model" />

<div class="d-flex gap-3">
	<a asp-action="Edit" asp-route-id="@Model.CategoryId" class="btn btn-primary">Edit</a>
	<a asp-action="Delete" asp-route-id="@Model.CategoryId" class="btn btn-danger">Delete</a>
	<a asp-action="Index" class="btn btn-secondary">Back</a>
</div>
