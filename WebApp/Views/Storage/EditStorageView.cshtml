@model StorageViewModel
@{
	ViewData["Title"] = "Edit Storage";
	Layout = "_DashBoardLayout";
}
<h1 class="mb-4">Edit Storage</h1>

<div class="text-danger mb-3">@TempData["Error"]</div>

<form asp-action="Edit" asp-route-id="@Model.ItemId" method="post">
	<partial name="Partials/_StoragePartial" model="Model" />

	<div class="d-flex gap-3">
		<button type="submit" class="btn btn-primary">Save</button>
		<a asp-action="Index" class="btn btn-secondary">Back</a>
	</div>
</form>
