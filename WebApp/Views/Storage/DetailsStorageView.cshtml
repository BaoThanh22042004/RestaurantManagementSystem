@model StorageViewModel
@{
    ViewData["Title"] = "Details Storage";
    Layout = "_DashBoardLayout";
}
<h1 class="mb-4">Details Storage</h1>

<div class="text-danger mb-3">@TempData["Error"]</div>

<partial name="Partials/_StoragePartial" model="Model" />

<div class="d-flex gap-3">
	<a asp-action="Edit" asp-route-id="@Model.ItemId" class="btn btn-primary">Edit</a>
	<a asp-action="Delete" asp-route-id="@Model.ItemId" class="btn btn-danger">Delete</a>
	<a asp-action="Index" class="btn btn-secondary">Back</a>
</div>

